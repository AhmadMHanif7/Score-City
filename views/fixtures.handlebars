<div class="scoreboardMain">
  <h1 class="leagueStandingsTitle"><span>GAME PREDICTIONS</span></h1>
  <div class="row">
    <div class="col-sm-offset-2 col-sm-8">

      {{#each fixturesData}}
        <div class="box">
          <div class="teamLogo1">
            <img src="{{homelogo}}" alt="" class="team1" />
            <p class="team1Name">{{hometeam}}</p>
          </div>
          <div class="scoreText">
            <p>{{homegoal}}-{{awaygoal}}</p>
          </div>
          <div class="teamLogo2">
            <img src="{{awaylogo}}" alt="" class="team1" />
            <p class="team1Name">{{awayteam}}</p>
          </div>
        </div>
      {{/each}}

    </div>
  </div>
</div>

<footer>
</footer>